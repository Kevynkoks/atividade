#include <stdio.h>

int main()
{
    
    float tempo, distancia, velocidade;
    
     scanf("%f",&distancia);
     scanf("%f",&velocidade);
     
     tempo=distancia/velocidade;
     
 
    
    printf("tempo:%.2f",tempo);

    return 0;
}